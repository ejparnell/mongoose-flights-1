<%- include('../partials/header') %>

<table>
    <thead>
        <tr>
            <th>Flight<br>Number</th>
            <th>Airline</th>
            <th>Airport</th>
            <th>Departs</th>
        </tr>
    </thead>
    <tbody>
        <% flights.forEach(function(flight){ %>
            <tr>
                <td><%= flight.flightNo %></td>
                <td><%= flight.airline %></td>
                <td><%= flight.airport %></td>
                <td><%= flight.departs %></td>
            </tr>
        <% }) %>
    </tbody>
</table>

<%- include('../partials/footer') %>